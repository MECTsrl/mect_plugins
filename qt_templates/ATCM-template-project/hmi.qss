	color: Silver;
}
/* QCheckBox && QLabel (net_conf, system_ini) */
page#net_conf  QCheckBox, page#net_conf  QLabel, 
page#system_ini  QCheckBox, page#system_ini  QLabel	{
	background-color: DimGray;
	color: rgb(255, 237, 0);
}
/* QTabWidget and Tab Bar (info, net_conf, system_ini) */
page#info QTabWidget::pane, 
page#net_conf QTabWidget::pane, 
page#system_ini QTabWidget::pane	{
        border-top: 1px solid rgb(87, 130, 193);
        border: 1px solid rgb(87, 130, 193);
	background-color: DimGray;
	color: rgb(255, 237, 0);
}
page#info QTabWidget::tab-bar, 
page#net_conf QTabWidget::tab-bar, 
page#system_ini QTabWidget::tab-bar	{
        left: 10px;
}
page#info QTabBar::tab, 
page#net_conf QTabBar::tab, 
page#system_ini QTabBar::tab	{
  	font-size: 14pt;
  	padding: 1px;
  	min-width:60px;
}
page#info QTabBar::tab:selected, 
page#net_conf QTabBar::tab:selected, 
page#system_ini QTabBar::tab:selected {
	qproperty-focusPolicy: NoFocus;
  	background: DimGray;
	color: Yellow;
  	margin-left: -1px;
  	margin-right: -1px;
}
page#info QTabBar::tab:!selected, 
page#net_conf QTabBar::tab:!selected, 
page#system_ini QTabBar::tab:!selected {
  	background: DarkGray;
	color: DarkRed;
    	margin-top: 4px; /* make non-selected tabs look smaller */
}
