/* Info Page QPlainTextEdit */
page#info QPlainTextEdit {
	border: 1px solid rgb(60, 60, 60);	
	border-radius: 2px;
	background-color: DimGray;
	color: rgb(255, 237, 0);
}
/* QProgressBar in page0 (startup), recipe, time_set  */
page#page0 QProgressBar, 
page#recipe QProgressBar, 
page#time_set QProgressBar	{
    border: 2px solid 	DeepSkyBlue;
    text-align: center;
    color: black;
}
page#page0 QProgressBar::chunk, 
page#recipe QProgressBar::chunk, 
page#time_set QProgressBar::chunk	{
    background-color: LightSkyBlue;
    margin: 1px;
}
/* Alarms QLineEdit */
page#alarms QLineEdit  {
    background: DarkGray;
    color: Yellow;
}
/* QListWidget in alarms, alarms_history, recipe_select, item_selector */
/* QTableWidget in Calendar, TimePopup, recipe, store */
page#alarms QListWidget, 
page#alarms_history QListWidget, 
page#recipe_select QListWidget, 
QDialog#item_selector QListWidget,
page#recipe QTableWidget, 
page#store QTableWidget, 
QDialog#Calendar QTableWidget, 
QDialog#TimePopup QTableWidget	{
    background-color: DarkGrey;
    alternate-background-color: LightGrey;
    show-decoration-selected: 1;
    gridline-color:  LightSkyBlue;
}
page#alarms QListWidget::item:selected, 
page#alarms_history QListWidget::item:selected, 
page#recipe_select QListWidget::item:selected, 
QDialog#item_selector QListWidget::item:selected,
page#recipe QTableWidget::item:selected, 
page#store QTableWidget::item:selected, 
QDialog#Calendar QTableWidget::item:selected, 
QDialog#TimePopup QTableWidget::item:selected		{
    border: 2px solid CadetBlue;
    background: red;
    color: yellow;
}
